@page "/payment/{IsFinance:bool}/{CarId:int}"
@inject NavigationManager NavigationManager

@code
{
    [EditorRequired, Parameter]
    public bool IsFinance { get; set; }

    [EditorRequired, Parameter]
    public int CarId { get; set; }

    protected override void OnInitialized()
    {
        base.OnInitialized();
        Console.WriteLine(IsFinance);
        if (IsFinance)

        {
            NavigationManager.NavigateTo($"/payment/{CarId}/payment");
        } else
        {
            NavigationManager.NavigateTo($"/payment/{CarId}/finance");
        }
    }
}

